{
  "name": "kudypoditys",
  "version": "1.0.0",
  "description": "Binary Studio Academy 2018 JS group project.",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && node server.js",
    "start-client": "node scripts/start.js",
    "start-api": "yarn sequelize db:migrate && yarn sequelize db:seed:all && nodemon --inspect=0.0.0.0:9228 server.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "sequelize": "sequelize --config=config/config.js --options-path=config/sequelize_options.js"
  },
